{
  "CN": "etcd",
  "hosts": [
    {% for item in groups['etcd'] %}"{{ hostvars[item]['ansible_ssh_host'] }}",
    {% endfor %}
    {% for item in groups['etcd'] %}"{{ hostvars[item]['inventory_hostname_short'] }}",
    {% endfor %}
    "127.0.0.1"
  ],
  "key": {
    "algo": "rsa",
    "size": 2048
  },
  "names": [
    {
      "C": "CN",
      "ST": "BeiJing",
      "L": "BeiJing",
      "O": "k8s",
      "OU": "System"
    }
  ]
}